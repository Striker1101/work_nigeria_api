{
  "name": "express-template",
  "version": "0.0.0",
  "engines": {
    "node": ">=18.12.1"
  },
  "private": true,
  "scripts": {
    "run-seed": "npx sequelize-cli db:seed:all",
    "make-seed": "npx sequelize-cli seed:generate --name demo-user",
    "migrate": "npx sequelize-cli db:migrate",
    "make-model": "npx sequelize-cli model:generate --name User --attributes firstName:string,lastName:string,email:string",
    "start": "node ./bin/www",
    "devstart": "nodemon ./bin/www",
    "serverstart": "SET DEBUG=postgres:* & npm run devstart"
  },
  "dependencies": {
    "async": "^3.2.4",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "~4.16.1",
    "express-session": "^1.17.3",
    "express-validator": "^6.14.2",
    "helmet": "^6.0.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^9.0.0",
    "luxon": "^3.2.1",
    "morgan": "~1.9.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.29.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.20",
    "sequelize-cli": "^6.6.0"
  }
}
